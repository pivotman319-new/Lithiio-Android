<?xml version='1.0' encoding='utf-8'?>
<widget id="com.milkgames.lithiio" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lithi.io</name>
    <description>
        A mobile application for the Lithi.io file sharing service.
    </description>
    <author email="memegames1337@gmail.com" href="https://milkgames.pw">
        MilkGames
    </author>
    <icon density="ldpi" src="icons/ldpi.png" />
    <icon density="mdpi" src="icons/mdpi.png" />
    <icon density="hdpi" src="icons/hdpi.png" />
    <icon density="xhdpi" src="icons/xhdpi.png" />
    <icon density="xxhdpi" src="icons/xxhdpi.png" />
    <icon density="xxxhdpi" src="icons/xxxhdpi.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <resource-file src="google-services.json" target="google-services.json" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="OverrideUserAgent" value="Lithiio-Android/1.1.1" />
    <preference name="AndroidLaunchMode" value="singleTask" />
</widget>
