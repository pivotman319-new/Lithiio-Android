<?xml version='1.0' encoding='utf-8'?>
<widget id="com.milkgames.lithiio" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lithi.io</name>
    <description>
        A mobile application for the Lithi.io file sharing service.
    </description>
    <author email="memegames1337@gmail.com" href="https://milkgames.pw">
        MilkGames
    </author>
    <icon density="ldpi" src="icons/ldpi.png" />
    <icon density="mdpi" src="icons/mdpi.png" />
    <icon density="hdpi" src="icons/hdpi.png" />
    <icon density="xhdpi" src="icons/xhdpi.png" />
    <icon density="xxhdpi" src="icons/xxhdpi.png" />
    <icon density="xxxhdpi" src="icons/xxxhdpi.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <resource-file src="google-services.json" target="google-services.json" />
        <preference name="AndroidLaunchMode" value="singleTask" />
    </platform>
    <plugin name="cordova-plugin-filechooser" spec="^1.0.1" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-filepath" spec="^1.2.0" />
    <plugin name="cordova-plugin-clipboard2" spec="^0.1.0" />
    <plugin name="com.napolitano.cordova.plugin.intent" spec="https://github.com/MilkGames/cordova-plugin-intent" />
    <plugin name="cordova-plugin-firebase-analytics" spec="^0.13.0">
        <variable name="FIREBASE_VERSION" value="11.6.+" />
    </plugin>
    <plugin name="cordova-support-google-services" spec="^1.1.0" />
    <engine name="android" spec="^7.0.0" />
</widget>
